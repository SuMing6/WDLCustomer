<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.homepage.HomePageMessageActivity"
    android:background="#F3F3F3">

    <RelativeLayout
        android:id="@+id/my_dz_add_tou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/dp_30"
        android:background="#fff"
        android:paddingBottom="@dimen/dp_9"
        >
        <ImageView
            android:id="@+id/homepage_message_back"
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_14"
            android:background="@drawable/back"
            android:layout_marginTop="@dimen/dp_4"
            android:layout_marginLeft="@dimen/dp_14"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textSize="@dimen/dp_16"
            android:text="新增收货地址"
            android:textColor="#FF333333"
            />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/homepage_message_LinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#fff"
        android:layout_marginTop="@dimen/dp_1">
        <ImageView
            android:layout_width="@dimen/dp_34"
            android:layout_height="@dimen/dp_34"
            android:src="@mipmap/homepage_message_dd"
            android:layout_marginTop="@dimen/dp_23"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginBottom="@dimen/dp_23"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_7"
                >

                <TextView
                    android:id="@+id/aaaaaaaaaaaaaaaaaaaa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="订单通知"
                    android:textColor="#333333"
                    android:textSize="@dimen/dp_16" />
                <TextView
                    android:id="@+id/home_page_message_dingdan_msg"
                    android:layout_below="@id/aaaaaaaaaaaaaaaaaaaa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="你的积分数量有变化哦"
                    android:textColor="#999999"
                    android:textSize="@dimen/dp_12"/>
                <TextView
                    android:id="@+id/home_page_message_dingdan_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9:00"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_24"
                    android:textSize="@dimen/dp_10"
                    android:textColor="#999999"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/homepage_message_LinearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#fff"
        android:layout_marginTop="@dimen/dp_1">
        <ImageView
            android:layout_width="@dimen/dp_34"
            android:layout_height="@dimen/dp_34"
            android:src="@mipmap/homepage_message_xx"
            android:layout_marginTop="@dimen/dp_23"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginBottom="@dimen/dp_23"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_7"
                >
                <TextView
                    android:id="@+id/bbbbbbbbbbbbbbbbbbbbbb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="系统消息"
                    android:textColor="#333333"
                    android:textSize="@dimen/dp_16"
                    android:layout_marginBottom="@dimen/dp_8"/>
                <TextView
                    android:layout_below="@+id/bbbbbbbbbbbbbbbbbbbbbb"
                    android:id="@+id/home_page_message_xitong_msg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#999999"
                    android:textSize="@dimen/dp_12"/>
                <TextView
                    android:id="@+id/home_page_message_xitong_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_24"
                    android:textSize="@dimen/dp_10"
                    android:textColor="#999999"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_1"
        android:background="#fff">
        <LinearLayout
            android:id="@+id/homepage_message_LinearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#fff">
            <ImageView
                android:layout_width="@dimen/dp_34"
                android:layout_height="@dimen/dp_34"
                android:src="@mipmap/homepage_message_kf"
                android:layout_marginTop="@dimen/dp_23"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_marginBottom="@dimen/dp_23"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_7"
                    >
                    <TextView
                        android:id="@+id/ccccccccccccccccccccccc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="客服信息"
                        android:textColor="#333333"
                        android:textSize="@dimen/dp_16"
                        android:layout_marginBottom="@dimen/dp_8"/>
                    <TextView
                        android:layout_below="@+id/ccccccccccccccccccccccc"
                        android:id="@+id/homepage_message_kf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="在线客服"
                        android:textColor="#999999"
                        android:textSize="@dimen/dp_12"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="9:00"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_24"
                        android:textSize="@dimen/dp_10"
                        android:textColor="#999999"
                        />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>